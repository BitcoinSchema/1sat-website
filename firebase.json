{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/wallet",
        "destination": "/wallet.html"
      },
      {
        "source": "/ordinals",
        "destination": "/ordinals.html"
      },
      {
        "source": "/inscribe",
        "destination": "/inscribe.html"
      },
      {
        "source": "/preview",
        "destination": "/preview.html"
      },
      {
        "source": "/bsv20",
        "destination": "/bsv20.html"
      },
      {
        "source": "/market/bsv20",
        "destination": "/market/bsv20.html"
      },
      {
        "source": "/market/listings",
        "destination": "/market/listings.html"
      },
      {
        "source": "/market/new",
        "destination": "/market/new.html"
      },
      {
        "source": "/market/new/**",
        "destination": "/market/new/[outPoint].html"
      },
      {
        "source": "/market/actibity",
        "destination": "/market/actibity.html"
      },
      {
        "source": "/market/bsv20/**",
        "destination": "/market/bsv20/[tick].html"
      },
      {
        "source": "/tx/**",
        "destination": "/tx/[outpoint].html"
      },
      {
        "source": "/collection/**",
        "destination": "/collection/[collectionId].html"
      },
      {
        "source": "/inscription/**",
        "destination": "/inscription/[inscriptionId].html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}